{
    "manifest_version": 3,
    "name": "Teto - Eesti keele assistent",
    "description": "Teto makes it easy to learn Estonian by defining unfamiliar words while reading articles.",
    "version": "0.0.1",
    "icons": {
        "32": "icon32.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}